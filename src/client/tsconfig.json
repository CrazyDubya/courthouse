{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/client",
    "rootDir": ".",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "node",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": ["vite/client"]
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "../server/**/*",
    "../dist/**/*",
    "../node_modules/**/*"
  ]
}